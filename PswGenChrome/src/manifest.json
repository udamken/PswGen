{
	"manifest_version" : 2,

	"name" : "PswGenChrome",
	"version" : "1.0.0",
	"browser_action" : {
		"default_icon" : "icon38.png",
		"default_popup" : "popup.html"
	},
	"content_scripts" : [
		{
			"matches" : [
				"<all_urls>"
			],
			"js" : [
				"content.js"
			]
		}
	],
	"permissions" : [
		"storage",
		"tabs"
	],
	"options_page" : "options.html"
}
